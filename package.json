  "license": "MIT",
  "engines": {
    "node": ">=20.x"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "framer-motion",
    "tailwindcss",
    "企业咨询",
    "文化咨询",
    "mumingter"
  ],
  "homepage": "https://mumingter.com/",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "prebuild": "yarn run typegen",
    "predev": "yarn run typegen",
    "typegen": "touch ./sanity/extract.json && sanity schema extract --enforce-required-fields --path=./sanity/extract.json || sanity typegen generate",
    "export": "next export",
    "deploy": "yarn run build && yarn run export && gh-pages -d out"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
